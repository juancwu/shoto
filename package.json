{
  "name": "shoto",
  "version": "0.2.1",
  "private": true,
  "scripts": {
    "dev": "pnpm migrate && next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "generate": "drizzle-kit generate:sqlite --out ./drizzle/migrations --breakpoints --schema=./src/server/schema.ts",
    "generate:custom": "drizzle-kit generate:sqlite --custom --out ./drizzle/migrations",
    "migrate": "tsx drizzle/migrate",
    "studio": "drizzle-kit studio --config=drizzle.config.ts --port 3333"
  },
  "dependencies": {
    "@clerk/nextjs": "^4.25.3",
    "@headlessui/react": "^1.7.17",
    "@heroicons/react": "^2.0.18",
    "@libsql/client": "^0.3.5",
    "class-variance-authority": "^0.7.0",
    "drizzle-orm": "^0.28.6",
    "nanoid": "^5.0.1",
    "next": "13.5.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "resend": "^1.1.0",
    "tailwind-merge": "^1.14.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^20.8.3",
    "@types/react": "^18.2.25",
    "@types/react-dom": "^18.2.11",
    "autoprefixer": "^10.4.16",
    "better-sqlite3": "^8.7.0",
    "dotenv": "^16.3.1",
    "drizzle-kit": "^0.19.13",
    "eslint": "^8.51.0",
    "eslint-config-next": "13.5.4",
    "postcss": "^8.4.31",
    "prettier": "^3.0.3",
    "prettier-plugin-tailwindcss": "^0.5.5",
    "tailwindcss": "^3.3.3",
    "tsx": "^3.13.0",
    "typescript": "^5.2.2"
  }
